{
  "name": "enlace-next",
  "version": "0.0.1-beta.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nxnom/enlace.git",
    "directory": "packages/next"
  },
  "bugs": {
    "url": "https://github.com/nxnom/enlace/issues"
  },
  "homepage": "https://github.com/nxnom/enlace/tree/main/packages/next#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --max-warnings 0",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build && npm run typecheck && npm run lint"
  },
  "dependencies": {
    "enlace-core": "workspace:*",
    "enlace": "workspace:*"
  },
  "peerDependencies": {
    "react": "^19"
  }
}
